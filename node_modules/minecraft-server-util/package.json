{
	"name": "minecraft-server-util",
	"version": "4.1.3",
	"description": "A Node.js library for Minecraft servers that can retrieve status, perform queries, and RCON into servers.",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"prepublishOnly": "npm run clean && npm run build && npm run lint && npm test",
		"build": "tsc",
		"clean": "rimraf build/",
		"lint": "eslint .",
		"test": "jest --detectOpenHandles --forceExit"
	},
	"keywords": [
		"minecraft",
		"java",
		"bedrock",
		"server",
		"status",
		"ping",
		"query",
		"rcon",
		"vote",
		"votifier"
	],
	"author": "Jacob Gunther",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@types/node": "^14.14.13",
		"@typescript-eslint/eslint-plugin": "^4.9.1",
		"@typescript-eslint/parser": "^4.9.1",
		"eslint": "^7.15.0",
		"jest": "^27.3.1",
		"rimraf": "^3.0.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"bugs": {
		"url": "https://github.com/PassTheMayo/minecraft-server-util"
	},
	"repository": {
		"url": "https://github.com/PassTheMayo/minecraft-server-util",
		"type": "git"
	},
	"dependencies": {
		"minecraft-motd-util": "^1.1.3"
	}
}